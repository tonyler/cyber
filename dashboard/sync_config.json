{
  "client": {
    "name": "Cosmos Labs",
    "project": "Cybernetics",
    "report_cadence": "monthly"
  },
  "sync": {
    "enabled": true,
    "interval_minutes": 1,
    "timeout_seconds": 30,
    "month_tab_format": "MM/YY",
    "summary_start_row": 50
  },
  "databases": {
    "stats_db": "../database/stats.csv",
    "links_db": "../database/links.csv"
  },
  "sheet_template": {
    "auto_create_enabled": true,
    "header_row": 1,
    "data_start_row": 2,
    "summary_start_row": 50,
    "x_columns": {
      "date": "A",
      "author": "B",
      "url": "C",
      "impressions": "D",
      "likes": "E",
      "comments": "F",
      "notes": "G",
      "total_impressions": "H"
    },
    "reddit_columns": {
      "date": "I",
      "author": "J",
      "url": "K",
      "views": "L",
      "upvotes": "M",
      "comments": "N",
      "notes": "O",
      "total_impressions": "P"
    },
    "x_summary_labels": [
      "Total Impressions",
      "Total Likes",
      "Total Posts"
    ],
    "reddit_summary_labels": [
      "Total Impressions",
      "Total Upvotes",
      "Total Posts"
    ]
  },
  "dashboard": {
    "page_load_warning_ms": 100,
    "query_warning_ms": 1,
    "default_month_format": "YYYY-MM"
  }
}
